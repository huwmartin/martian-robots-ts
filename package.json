{
  "name": "martian-robots",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn lint && yarn build-ts",
    "start": "node dist/main.js",
    "test": "jest --forceExit --verbose",
    "coverage": "yarn test --coverage",
    "watch-test": "yarn test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "yarn tslint",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "yarn tslint --fix"
  },
  "devDependencies": {
    "@types/node": "^10.12.9",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "node": "^11.1.0"
  }
}
